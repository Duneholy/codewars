   "source": [
    "#Find the last element of the given argument(s).\n",
    "\n",
    "#Examples\n",
    "#last([1, 2, 3, 4]) ==>  4\n",
    "#last(\"xyz\")        ==> \"z\"\n",
    "#last(1, 2, 3, 4)   ==>  4"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "def last(*a):\n",
    "    return a[-1] if isinstance(a[-1], int) else a[-1][-1]"
   ]
  },
