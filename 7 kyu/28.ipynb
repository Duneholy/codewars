    "def calculate_years(principal, interest, tax, desired):\n",
    "    years = 0\n",
    "    while principal < desired:\n",
    "        principal = (principal+(principal*interest))-(principal*interest)*tax\n",
    "        years +=1\n",
    "    return years"
