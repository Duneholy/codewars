   "source": [
    "#Given an integer as input, can you round it to the next (meaning, \"higher\") 5?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def round_to_next5(n):\n",
    "    while n%5!=0:\n",
    "        n += 1\n",
    "    return n"
   ]
