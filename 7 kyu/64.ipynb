    "def sum_of_n(n):\n",
    "    res = []\n",
    "    s = 0\n",
    "    for i in range (0,abs(n)+1):\n",
    "        s += i\n",
    "        if n>0:\n",
    "            res.append(s)\n",
    "        else:\n",
    "            res.append(-s)\n",
    "    return res"
   ]
