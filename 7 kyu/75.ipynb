   "source": [
    "def ordered_count(input):\n",
    "    samp = set(input)\n",
    "    res = []\n",
    "    for i in sorted(samp, key=input.index):\n",
    "        res.append ((i, input.count(i)))\n",
    "    return res"
   ]
  }
